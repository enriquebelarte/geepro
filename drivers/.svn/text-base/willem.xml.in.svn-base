<notebook name="HW Test">
    <table dim="2,3">
	<frame label="ZIF32" pos="0,1, 0,3" align="50, 0" expandy="true" filly="true" expandx="true" fillx="true">
	    <hbox>
		<frame shadow="etched_in">
		    <vbox>
			<chbutton id="pin_1"  label="1-Vpp"/>
			<chbutton id="pin_2"  label="2-A16"/>
			<chbutton id="pin_3"  label="3-A15"/>
			<chbutton id="pin_4"  label="4-A12"/>
			<chbutton id="pin_5"  label="5-A7" />
			<chbutton id="pin_6"  label="6-A6" />
			<chbutton id="pin_7"  label="7-A5" />
			<chbutton id="pin_8"  label="8-A4" />
			<chbutton id="pin_9"  label="9-A3" />
			<chbutton id="pin_10" label="10-A2"/>
			<chbutton id="pin_11" label="11-A1"/>
			<chbutton id="pin_12" label="12-A0"/>
			<chbutton id="pin_13" label="13-D0"/>
			<chbutton id="pin_14" label="14-D1"/>
			<chbutton id="pin_15" label="15-D2"/>
			<chbutton id="pin_16" sensitive="false" label="16-GND"/>
		    </vbox>    
		</frame>
		<frame shadow="etched_in">
		    <vbox>
			<chbutton id="pin_32" place="right" label="Vcc-32"/>
			<chbutton id="pin_31" place="right" label="WE-31" />
			<chbutton id="pin_30" place="right" label="A17-30"/>
			<chbutton id="pin_29" place="right" label="A14-29"/>
			<chbutton id="pin_28" place="right" label="A13-28"/>
			<chbutton id="pin_27" place="right" label="A8-27" />
			<chbutton id="pin_26" place="right" label="A9-26" />
			<chbutton id="pin_25" place="right" label="A11-25"/>
			<chbutton id="pin_24" place="right" label="OE-24" />
			<chbutton id="pin_23" place="right" label="A10-23"/>
			<chbutton id="pin_22" place="right" label="CE-22" />
			<chbutton id="pin_21" place="right" label="D7-21" />
			<chbutton id="pin_20" place="right" label="D6-20" />
			<chbutton id="pin_19" place="right" label="D5-19" />
			<chbutton id="pin_18" place="right" label="D4-18" />
			<chbutton id="pin_17" place="right" label="D3-17" />
		    </vbox>
		</frame>
	    </hbox>
	</frame>
	<frame label="Set Address/Data" align="50, 50" pos="1,2, 0,1" expandx="true" fillx="true" filly="true" spacex="5">
	    <table dim="2,3">
	        <spinbutton pos="1,2, 0,1" id="address" type="hex" max="0x3ffff" size="50, 25" fillx="true" expandx="true" spacex="10" />
	        <label text="Address:" pos="0,1, 0,1" fillx="true" align="0, 0" spacex="10" />
	        <spinbutton pos="1,2, 1,2" id="data" type="hex" max="0xff" size="50, 25" fillx="true" expandx="true" spacex="10" />
	        <label text="Data:" pos="0,1, 1,2" fillx="true" align="0, 0" spacex="10" />
	        <button pos="0,2, 2,3" id="rd_data_bt" label="Read data D0..D7" fillx="true" expandx="true" filly="true" expandy="true" spacey="10" spacex="10"/>
	    </table>
	</frame>
	<frame label="Pamiec 93Cxx" pos="1,2, 1,2" align="50, 50" expandx="true" fillx="true" spacex="5">
	    <table dim="2,4">
		<chbutton id="pin_cs" pos="0,1, 0,1" label="1-CS" fillx="true" spacex="5" />
		<chbutton id="pin_clk" pos="0,1, 1,2" label="2-CLK" fillx="true" spacex="5" />
		<chbutton id="pin_di" pos="0,1, 2,3" label="3-DI" fillx="true" spacex="5" spacey="5" />
		<entry id="pin_do_et" editable="false" pos="0,1, 3,4" fillx="true" size="1" spacex="5" />
		<button id="clk_sqw" pos="1,2, 1,2" label="CLK SQW" expandx="true" fillx="true" spacex="5" />
		<button id="pin_do_bt" pos="1,2, 3,4" label="4-DO" expandx="true" fillx="true" spacex="5" spacey="5" />
	    </table>	
	</frame>
	<dipswitch name="Dip switch" len="12" value="0xaaaaa" pos="1,2, 2,3" />
    </table>
</notebook>

</gui>


